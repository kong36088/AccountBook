<!--index.wxml-->
<view class="page">

  <view class="page_hd">
    <button class="btn-yellow">+</button>
    <mp-cells ext-class="my-cells" title="总概览">
      <mp-cell value="总金额" footer="{{total_fund}}"></mp-cell>
      <mp-cell value="流动资金" footer="{{liqiuid_fund}}"></mp-cell>
      <mp-cell value="非流动资金" footer="{{illiqiuid_fund}}"></mp-cell>
    </mp-cells>
  </view>

  <view class="page__bd">
    <!-- <view class="divdeLine"></view> -->
    <mp-cells ext-class="my-cells" title="明细">
      <mp-cell value="招行" footer="1234元"></mp-cell>
    </mp-cells>
  </view>

  <view class="page__ft">
    <mp-tabbar style="position:fixed;bottom:0;width:100%;left:0;right:0;" list="{{tarbarPros}}" bindchange="navbarChanged"></mp-tabbar>
  </view>

</view>